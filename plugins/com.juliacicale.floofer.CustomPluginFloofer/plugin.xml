<?xml version='1.0' encoding='utf-8'?>
<plugin id="com.juliacicale.floofer.CustomPluginFloofer" version="0.0." xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
	<name>CustomPluginFloofer</name>
	<description>a custom android plugin delivering you a pup to your console!</description>
	<js-module name="CustomPluginFloofer" src="www/CustomPluginFloofer.js">
		<clobbers target="window.CustomPluginFloofer" />
	</js-module>
	<platform name="android">
		<config-file parent="/*" target="res/xml/config.xml">
			<feature name="CustomPluginFloofer">
				<param name="android-package" value="com.juliacicale.floofer.CustomPluginFloofer.CustomPluginFloofer" />
			</feature>
		</config-file>
		<config-file parent="/*" target="AndroidManifest.xml">
			
		</config-file><source-file src="src/android/CustomPluginFloofer.java" target-dir="src/com/juliacicale/floofer/CustomPluginFloofer/CustomPluginFloofer" />
	</platform>
</plugin>